  "scripts": {
    "cmd": "./cmd.sh",
    "start": "node dist/main",
    "start:worker": "node dist/worker",
    "start:repl": "node dist/repl",
    "start:cli": "NODE_ENV=cli node dist/cli",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest -c jest.config.ts -w 100% --selectProjects unit",
    "test:e2e": "jest -c jest.config.ts --maxWorkers=1 --selectProjects e2e",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "cli": "./cmd.sh cli",
    "dev": "concurrently -k --handle-input --kill-signal=SIGINT -n app,worker -c blue,green 'npm run dev:app' 'npm run dev:worker'",
    "dev:app": "npx nest start --watch",
    "dev:worker": "nodemon --exec 'ts-node -r tsconfig-paths/register ./src/worker.ts'",
    "debug": "nest start --debug --watch",
    "dev:email": "email dev --dir ./src/infra/global/email/template --port 3001",
    "repl": "./cmd.sh repl",
    "lang": "npx typesafe-i18n --no-watch",
    "typesafe-i18n": "typesafe-i18n"
  },
